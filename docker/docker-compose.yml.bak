e1:
  image: elasticsearch:2.3.5
  volumes:
    - ./e1/config/:/usr/share/elasticsearch/config/
  ports: 
    - 9201:9200
e2:
  image: elasticsearch:2.3.5
  volumes:
    - ./e2/config/:/usr/share/elasticsearch/config/
    - ./e2/data/:/data/
  ports: 
    - 9202:9200
  command: elasticsearch
e3:
  image: elasticsearch:2.3.5
  volumes:
    - ./e3/config/:/usr/share/elasticsearch/config/
    - ./e3/data/:/data/
  ports: 
    - 9203:9200
  command: elasticsearch
